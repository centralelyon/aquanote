{
  "name": "config",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@playwright/test": "^1.53.2",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "d3": "^7.9.0",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "http-server": "^14.1.1",
    "jquery": "^3.6.0",
    "jsdom": "^26.1.0",
    "numeric": "^1.2.6",
    "sinon": "^20.0.0",
    "vitest": "^2.1.9"
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest test/unit",
    "test:integration": "vitest test/integration",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:all": "npm run test && npm run test:e2e",
    "dev": "http-server . -p 3000 -c-1 --cors",
    "serve": "http-server . -p 3000 -c-1 --cors",
    "lint": "eslint",
    "start": "npx electron",
    "build:windows": "electron-builder -w",
    "build:mac": "electron-builder -m",
    "build:linux": "electron-builder -l"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "node-fetch": "^3.3.2",
    "perspective-transform": "^1.1.3"
  },
  "build": {
    "appId": "com.enzo.annotation",
    "productName": "Neptune",
    "icon": "build/icons",
    "files": [
      "**/*",
      "!html/**",
      "!latex/**",
      "!test/**",
      "!courses_natation_local/**",
      "!node_modules/**",
      "!*.ipynb",
      "!local.py",
      "!docs/**",
      "!*.md",
      "!*.csv",
      "!*.log",
      "!requirements.txt",
      "!*.mjs",
      "!Doxyfile",
      "!.mypy_cache/**",
      "!test/setup.js",
      "!pipeline/**",
      "!codes_python_divers/**",
      "!*.db",
      "!.conda/**",
      "!.mypy_cache/**",
      "!.git/**"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/win/icon.ico"
    }
  }
}
